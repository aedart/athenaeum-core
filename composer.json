{
    "name": "aedart/athenaeum-core",
    "description": "Athenaeum core package",
    "keywords": [
        "Athenaeum",
        "Core",
        "Application"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "psr/log": "^1.1",
        "symfony/console": "4.4.*",
        "vlucas/phpdotenv": "^3.3",
        "illuminate/cache": "^6.15",
        "illuminate/filesystem": "^6.15",
        "aedart/athenaeum-config": "^4.0.x-dev",
        "aedart/athenaeum-console": "^4.0.x-dev",
        "aedart/athenaeum-container": "^4.0.x-dev",
        "aedart/athenaeum-contracts": "^4.0.x-dev",
        "aedart/athenaeum-dto": "^4.0.x-dev",
        "aedart/athenaeum-events": "^4.0.x-dev",
        "aedart/athenaeum-service": "^4.0.x-dev",
        "aedart/athenaeum-support": "^4.0.x-dev",
        "aedart/athenaeum-utils": "^4.0.x-dev"
    },
    "require-dev": {
        "symfony/var-dumper": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Core\\": "src"
        },
        "files": [
            "helpers/paths.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
